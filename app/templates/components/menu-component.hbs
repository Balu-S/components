  {{#if subMenuObj.length}}
  <div class="ui dropdown item {{if isShowSubMenu 'active visible' ''}}">
   {{menuItem.name}}
    <i class="dropdown icon"></i>
      <div class="menu {{if isShowSubMenu 'transition visible' 'transition hidden'}}" id="submenudiv" style="display:{{if isShowSubMenu 'block' 'none'}}">
        {{#each subMenuObj as | item |}}
           <a class="item"> {{item.subname}}
             <i class="icon" style="color:#949191;font-size:15px;padding:2px 0px 0px 35px">{{item.type}}</i>
           </a>
        {{/each}}
      </div>
 </div>
{{else}}
    <a class="item">
      {{menuItem.name}}
    </a>
{{/if}}
